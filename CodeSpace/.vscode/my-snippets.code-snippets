            "\tvector<int> res;",
            "\tfor(int i = 0; i < A.size();i++){",
            "\t\ttem =A[i] - tem;",
            "\t\tif(i< B.size()) tem = tem - B[i];",
            "\t\tres.push_back((10+tem)%10);",
