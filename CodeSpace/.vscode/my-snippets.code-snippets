            "\tfor(int i = A.size()-1;i>=0;i--){",
            "\t\tmod = 10*mod +A[i];",
            "\t\tres.push_back(mod/b);",
            "\t\tmod%=b;",
            "\t}",
            "\treverse(res.begin(),res.end());",
            "\twhile(res.size()>1&&res.back()==0)res.pop_back();",
            "\treturn res;",
            "}",
