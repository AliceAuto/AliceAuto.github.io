            "\tvector <int> res;",
            "\tif(A.size()>B.size())return add(B,A);",
            "\tfor(int i =0 ; i <B.size();i++){",
            "\t\ttem += B[i];",
            "\t\tif(i<A.size()) tem+=A[i];",
            "\t\tres.push_back(tem%10);",
            "\t\ttem = tem/10; ",
            "\t}",
            "\tif(tem)res.push_back(tem);",
            "\treturn res;",
            "}",
