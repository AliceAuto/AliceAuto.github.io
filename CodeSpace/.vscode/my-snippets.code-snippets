            "            dd = 1, mm++;",
            "            if (mm > 12) mm = 1, yy++;",
            "        } else dd++;",
            "    }",
            "    bool isSame(Date d) {",
            "        return d.getDay() == dd && d.getMonth() == mm && d.getYear() == yy;",
            "    }",
            "    void print() const { printf(\"%d-%02d-%02d\\n\", yy, mm, dd); }",
            "    int getYear() const { return yy; }",
            "    int getMonth() const { return mm; }",
            "    int getDay() const { return dd; }",
            "};",
            "/*",
            "",
            "example :",
            "",
