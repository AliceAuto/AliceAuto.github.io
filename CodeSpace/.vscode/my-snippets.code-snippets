            "            self.lst = [-1] * n",
            "        ",
            "        def append(self, i, j):",
            "            self.pre.append(self.lst[i])",
            "            self.lst[i] = len(self.cur)",
            "            self.cur.append(j)",
            "        ",
            "        def iterate(self, i):",
            "            tmp = self.lst[i]",
            "            while tmp != -1:",
            "                yield self.cur[tmp]",
            "                tmp = self.pre[tmp]"
